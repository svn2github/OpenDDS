// -*- MPC -*-
// $Id$

project : orbsvcslib, dcps_ts_defaults {
  // override the orbsvcslib default library output directory
  libout      = $(DDS_ROOT)/lib
  libs        += TAO_DdsDcps
  after       += DdsDcps
  libpaths    += $(DDS_ROOT)/lib

  includes    += $(DDS_ROOT)
  idlflags    += -Gdcps -I $(DDS_ROOT)

  // DDS is not compatible with an exceptions=0 build
  requires    += exceptions
}
