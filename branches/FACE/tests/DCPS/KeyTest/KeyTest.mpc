
project(DDS*idl): dcps_test_lib {
  idlflags      += -Wb,export_include=KeyTest_export.h \
                   -Wb,export_macro=KeyTest_Export
  dcps_ts_flags += -Wb,export_macro=KeyTest_Export
  dynamicflags  += KEYTEST_BUILD_DLL

  specific (vc9, vc10) {
    compile_flags += /bigobj
  }

  TypeSupport_Files {
    KeyTest.idl
    KeyTest2.idl
    BoundTest.idl
    BoundTest2.idl
  }

  Idl_Files {
    KeyTest.idl
    KeyTest2.idl
    BoundTest.idl
    BoundTest2.idl
  }
}


project(DDS*IsBounded) : dcpsexe, dcps_tcp, dcps_rtps, dcps_udp, dcps_multicast {

  exename   = IsBounded
  after    += DDS*idl
  libs     += DDS*idl

  Idl_Files {
  }

  Source_Files {
