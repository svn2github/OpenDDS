project(*Publisher): dcpsexe, dcps_tcp {
  exename = FooTest3_publisher
  after       += DcpsFooType3
  libs     +=  DcpsFooType3
  libpaths += ../FooType3

  Source_Files {
    pub_main.cpp
    PubDriver.cpp
