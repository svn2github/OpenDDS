project(*Publisher): dcpsexe, dcps_tcp {
  exename = FooTest3NoKey_publisher
  after    += DcpsFooType3NoKey
  libs     +=  DcpsFooType3NoKey
  libpaths += ../FooType3NoKey

  Source_Files {
    pub_main.cpp
    PubDriver.cpp
    InstanceDataMap.cpp
    Writer.cpp
  }
}

project(*Subscriber): dcpsexe, dcps_tcp {
  exename = FooTest3NoKey_subscriber
  after    += DcpsFooType3NoKey *Publisher
  libs     += DcpsFooType3NoKey
  libpaths += ../FooType3NoKey

  Source_Files {
    sub_main.cpp
    SubDriver.cpp
    DataReaderListener.cpp
  }
}
