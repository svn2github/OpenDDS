project(ManyTopicTestPublisher): dcpsexe, dcps_tcp, dcps_rtps_udp, dds_model {
  exename   = publisher
  libpaths += ../ManyTopicTypes
  libpaths += ../common
  libs     +=  common ManyTopicTypes
  after    += common ManyTopicTypes

  Header_Files {
    common.h
  }

  Source_Files {
    publisher.cpp
    Writer.cpp
  }
}


project(ManyTopicTestSubscriber): dcpsexe, dcps_tcp, dcps_rtps_udp {
  exename   = subscriber
  libpaths += ../ManyTopicTypes
  libpaths += ../common
  libs     +=  common ManyTopicTypes
  after    += common ManyTopicTypes

  Header_Files {
    common.h
    DataReaderListener.h
  }

  Source_Files {
    subscriber.cpp
  }
}

